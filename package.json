{
  "name": "electron-starter",
  "version": "0.0.1",
  "description": "electron-starter",
  "main": "src/main",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron src/main --serve & npm run watch",
    "watch": "webpack --config ./config/webpack/webpack.development --watch",
    "build": "webpack --config ./config/webpack/webpack.development",
    "build:production": "webpack --config ./config/webpack/webpack.production",
    "lint-scripts": "eslint -f stylish 'config/**/*.js' 'src/**/*.js' 'src/**/*.ts'",
    "lint-views": "pug-lint src/renderer/**/*.pug",
    "lint": "npm run lint-scripts && npm run lint-views",
    "package": "rm -rf src/build && npm run build:production && electron-builder build --publish never"
  },
  "devDependencies": {
    "@angular/common": "^11.2.3",
    "@angular/compiler": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/forms": "^11.2.3",
    "@angular/platform-browser": "^11.2.3",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@angular/router": "^11.2.3",
    "@types/core-js": "^2.5.3",
    "@types/lodash": "^4.14.153",
    "@types/node": "^14.0.5",
    "@types/toastr": "^2.1.38",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "bootstrap": "^4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^5.1.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.2.0",
    "eslint": "^7.20.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^1.3.9",
    "progress-bar-webpack-plugin": "^2.1.0",
    "pug": "^3.0.0",
    "pug-html-loader": "^1.1.5",
    "pug-lint": "^2.6.0",
    "raw-loader": "^4.0.2",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "toastr": "^2.1.4",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-notifier": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderMac/electron-starter.git"
  },
  "author": "Alexander Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMac/electron-starter/issues"
  },
  "homepage": "https://github.com/AlexanderMac/electron-starter#readme"
}
